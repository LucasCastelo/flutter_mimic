{
	"name": "flutter_mimic",
	"displayName": "Flutter Mimic",
	"description": "A simple extension to mimic Flutter lib folder into test folder.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.59.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onStartupFinished",
		"onCommand:facily-scaffold.helloWorld",
		"onCommand:facily-scaffold.createScaffold"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "facily-scaffold.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "facily-scaffold.createScaffold",
				"title": "Facily - Create Module Scaffold"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.59.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"eslint": "^7.27.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	}
}
